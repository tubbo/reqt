    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "eslint": "^4.5.0",
    "eslint-loader": "^1.9.0",
    "jsdoc": "^3.5.4",
    "minami": "^1.2.3",
    "mocha": "^3.5.0",
    "node-fetch": "^1.7.2",
    "script-type-module": "^1.0.1",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "babel-plugin-add-module-exports"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "eol-last": [
